                        <Label text="Last Name" GridPane.rowIndex="4" />
                        <Label text="City" GridPane.rowIndex="5" />
                        <TextField fx:id="cityText" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                        <TextField fx:id="lNameText" GridPane.columnIndex="1" GridPane.rowIndex="4" />
