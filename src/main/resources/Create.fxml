                          <opaqueInsets>
                              <Insets />
                          </opaqueInsets>
                      </GridPane>
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="38.0" prefWidth="252.0">
               <children>
                  <Button fx:id="createButton" layoutX="11.0" layoutY="7.0" mnemonicParsing="false" onAction="#createButtonAction" prefHeight="25.0" prefWidth="115.0" text="Create User" />
                  <Button fx:id="closeButton" layoutX="140.0" layoutY="7.0" mnemonicParsing="false" onAction="#closeButtonAction" prefHeight="25.0" prefWidth="115.0" text="Close" />
